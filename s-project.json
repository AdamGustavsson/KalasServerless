{
  "name": "serverless-graphql",
  "version": "0.0.1",
  "profile": "serverless-v0.4.2",
  "location": "https://github.com/...",
  "author": "",
  "description": "A Slick New Serverless Project",
  "custom": {
    "meta": {
      "name": "${meta_bucket}",
      "region": "${meta_bucket_region}"
    },
    "client": {
      "bucketName": "graphql-test-project.${stage}.${region}"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-meta-sync",
    "serverless-cors-plugin",
    "serverless-client-s3",
    "serverless-offline",
    "./plugins/setupdb.js"
  ]
}